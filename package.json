{
    "name": "gympass-api",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "dev": "tsx --watch src/server.ts",
        "start": "node dist/server.js",
        "build": "tsup src/server.ts --format esm --dts --out-dir dist",
        "db:init": "prisma init",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:migrate:edit": "prisma migrate dev --create-only",
        "db:studio": "prisma studio",
        "test": "vitest run --coverage",
        "test:unit": "vitest run --project unit --coverage",
        "test:e2e": "vitest run --project e2e --no-file-parallelism --coverage",
        "test:db": "dotenv -e .env.test prisma studio",
        "test:watch": "vitest watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fastify/swagger": "9.5.1",
        "@prisma/client": "6.14.0",
        "@scalar/fastify-api-reference": "1.34.2",
        "bcryptjs": "3.0.2",
        "dayjs": "1.11.13",
        "dotenv": "17.2.1",
        "fastify": "5.5.0",
        "fastify-type-provider-zod": "5.0.3",
        "zod": "4.0.17"
    },
    "devDependencies": {
        "@rueda.dev/config": "1.2.7",
        "@types/bcryptjs": "2.4.6",
        "@types/node": "24.3.0",
        "@types/supertest": "6.0.3",
        "@vitest/coverage-v8": "3.2.4",
        "dotenv-cli": "10.0.0",
        "prisma": "6.14.0",
        "supertest": "7.1.4",
        "tsup": "8.5.0",
        "tsx": "4.20.4",
        "typescript": "5.9.2",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "3.2.4"
    }
}
